<h1>Users#edit</h1>
<p>Find me in app/views/users/edit.html.erb</p>

<div>
    <h1>Editing Book</h1>
    <%= form_with model: @user do |f| %>
        name<br>
        <%= f.text_field :name %><br>
        introduction<br>
        <%= f.text_area :introduction %><br>
        image<br>
        <%= f.file_field :profile_image %><br>
        <%= f.submit "update" %>
    <% end %>
</div>